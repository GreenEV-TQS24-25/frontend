    "components": {
        "schemas": {
            "Vehicle": {
                "required": [
                    "brand",
                    "connectorType",
                    "licensePlate",
                    "model"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "brand": {
                        "maxLength": 50,
                        "minLength": 0,
                        "type": "string"
                    },
                    "model": {
                        "maxLength": 50,
                        "minLength": 0,
                        "type": "string"
                    },
                    "licensePlate": {
                        "maxLength": 20,
                        "minLength": 0,
                        "type": "string"
                    },
                    "connectorType": {
                        "type": "string",
                        "enum": [
                            "SAEJ1772",
                            "MENNEKES",
                            "CHADEMO",
                            "CCS"
                        ]
                    }
                }
            },
            "User": {
                "required": [
                    "email",
                    "name",
                    "password"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "name": {
                        "maxLength": 255,
                        "minLength": 0,
                        "type": "string"
                    },
                    "email": {
                        "maxLength": 255,
                        "minLength": 0,
                        "type": "string"
                    },
                    "password": {
                        "maxLength": 255,
                        "minLength": 0,
                        "type": "string"
                    },
                    "role": {
                        "type": "string",
                        "enum": [
                            "USER",
                            "OPERATOR"
                        ]
                    }
                }
            },
            "ChargingStation": {
                "required": [
                    "lat",
                    "lon",
                    "name"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "name": {
                        "maxLength": 100,
                        "minLength": 0,
                        "type": "string"
                    },
                    "lat": {
                        "type": "number"
                    },
                    "lon": {
                        "type": "number"
                    },
                    "operator": {
                        "$ref": "#/components/schemas/User"
                    },
                    "photoUrl": {
                        "maxLength": 255,
                        "minLength": 0,
                        "type": "string"
                    }
                }
            },
            "ChargingSpot": {
                "required": [
                    "powerKw",
                    "pricePerKwh"
                ],
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "station": {
                        "$ref": "#/components/schemas/ChargingStation"
                    },
                    "powerKw": {
                        "type": "number"
                    },
                    "pricePerKwh": {
                        "type": "number"
                    },
                    "chargingVelocity": {
                        "type": "string",
                        "enum": [
                            "NORMAL",
                            "FAST",
                            "FASTPP"
                        ]
                    },
                    "connectorType": {
                        "type": "string",
                        "enum": [
                            "SAEJ1772",
                            "MENNEKES",
                            "CHADEMO",
                            "CCS"
                        ]
                    },
                    "state": {
                        "type": "string",
